class Tipo1():
    def Metodo(self):
        print("metodo del tipo1")

class Tipo2():
    def Metodo(self):
        print("metodo del tipo2")

class Tipo3():
    def Metodo(self):
        print("metodo del tipo3")

#los tres son clases diferentes pero tienen un metodo con el mismo nombre

def Elector(FuncionElectora):
    FuncionElectora.Metodo()

Objetoelector = Tipo2()#esto es el polimorfismo, poder elegir que metodo elegir de acuerdo a la clase jugando con que tienen el mismo nombre
Elector(Objetoelector)    